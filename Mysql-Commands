
------------------------------------------------------------------------------------------------------------------

Create database graphql;

------------------------------------------------------------------------------------------------------------------

show databases;

------------------------------------------------------------------------------------------------------------------

use graphql;

------------------------------------------------------------------------------------------------------------------


CREATE TABLE property (
    id INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
    address VARCHAR(100),
    event VARCHAR(100),
    name VARCHAR(100)	
);

------------------------------------------------------------------------------------------------------------------



INSERT INTO `property` (`id`, `address`, `event`, `name`) VALUES ('1', 'Plot no 5', 'sell', 'digitalCode');

